include <iostream>
#include <conio.h>
#include <cmath>
using namespace std;

//SodiumNitrogenZinc//
int main ()
{
	int nnz;
	
	cout << "Multiplication Table \n";
	cout << "Enter a number: "; cin >> nnz;
	cout << "Multiplication Table of " << nnz << endl;
	
	for (int i = 1; i <= 10; ++i) 
        cout << nnz << " * " << i << " = " << nnz * i << endl;
	
	return 0;
	
}
